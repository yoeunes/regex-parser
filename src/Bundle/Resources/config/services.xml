<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <defaults autowire="true" autoconfigure="true" public="false" />

        <service id="RegexParser\NodeVisitor\ExplainVisitor" />
        <service id="RegexParser\NodeVisitor\ComplexityScoreVisitor" />
        <service id="RegexParser\NodeVisitor\HtmlExplainVisitor" />

        <service id="regex_parser.collector" class="RegexParser\Bundle\DataCollector\RegexCollector">
            <tag name="data_collector"
                 template="@RegexParser/Profiler/regex_panel.html.twig"
                 id="regex"
                 priority="270" />
            <argument type="service" id="RegexParser\NodeVisitor\ExplainVisitor" />
            <argument type="service" id="RegexParser\NodeVisitor\ComplexityScoreVisitor" />
        </service>

        </services>
</container>
