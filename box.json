 {
  "compression": "GZ",
  "directories": [
    "src",
    "vendor"
  ],
  "files": [
    "composer.json",
    "LICENSE"
  ],
  "finder": [
    {
      "name": "regex",
      "in": "bin"
    }
  ],
  "blacklist": [
    ".github",
    "art",
    "benchmarks",
    "bin/build",
    "bin/php-cs-fixer",
    "bin/phpbench",
    "bin/phpcs",
    "bin/phplint",
    "bin/phpstan",
    "bin/rector",
    "bin/regex.phar",
    "corpus",
    "docs",
    ".editorconfig",
    ".gitattributes",
    ".gitignore",
    ".gitmessage",
    ".php-cs-fixer.dist.php",
    ".phplint.yml",
    ".php-version",
    "phpbench.yml",
    "phpbench.json",
    "phpstan.dist.neon",
    "rector.php",
    "phpunit.dist.xml",
    "taskfile.dist.yaml",
    "box.json",
    "phpstan-baseline.neon",
    "CHANGELOG.md",
    "regex.dist.json",
    "CODE_OF_CONDUCT.md",
    "CONTRIBUTING.md",
    "SECURITY.md",
    "tests",
    "tools",
    ".task",
    ".cache"
  ]
}
