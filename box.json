{
    "compression": "GZ",
    "directories": [
        "src",
        "vendor"
    ],
    "files": [
        "composer.json",
        "LICENSE"
    ],
    "finder": [
        {
            "name": "regex",
            "in": "bin"
        }
    ],
    "blacklist": [
        ".cache",
        ".editorconfig",
        ".gitattributes",
        ".github",
        ".gitignore",
        ".gitmessage",
        ".php-cs-fixer.dist.php",
        ".php-version",
        ".phplint.yml",
        ".task",
        "CHANGELOG.md",
        "CODE_OF_CONDUCT.md",
        "CONTRIBUTING.md",
        "README.md",
        "SECURITY.md",
        "art",
        "benchmarks",
        "bin/build",
        "bin/php-cs-fixer",
        "bin/phpbench",
        "bin/phpcs",
        "bin/phplint",
        "bin/phpstan",
        "bin/rector",
        "bin/regex.phar",
        "box.json",
        "corpus",
        "docs",
        "phpbench.json",
        "phpbench.yml",
        "phpstan-baseline.neon",
        "phpstan.dist.neon",
        "phpunit.dist.xml",
        "rector.php",
        "regex.dist.json",
        "taskfile.dist.yaml",
        "tests",
        "tools"
    ]
}
